// Function to check if a number is even
function isEven(number) {
    return number % 2 === 0;
}

// Function to check if a number is odd
function isOdd(number) {
    return number % 2 !== 0;
}

// Function that takes another function as a parameter 
function checkNumber(func, number) {
    return func(number);
}

let resultEven = checkNumber(isEven, 4);  // Check if 4 is even
let resultOdd = checkNumber(isOdd, 5);    // Check if 5 is odd

console.log(resultEven);  // Output: true
console.log(resultOdd);   // Output: true
